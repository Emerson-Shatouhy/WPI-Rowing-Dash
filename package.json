{
  "private": true,
  "scripts": {
    "build": "turbo build",
    "clean": "turbo clean",
    "dev": "turbo dev --no-cache  --continue",
    "format": "prettier --write --plugin=prettier-plugin-tailwindcss --ignore-unknown",
    "lint": "turbo lint",
    "test": "turbo test",
    "typecheck": "turbo typecheck",
