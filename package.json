    "react-dom": "^18.2.0",
    "react-https-redirect": "^1.1.0",
    "react-icons": "^4.11.0",
    "react-router-dom": "^6.18.0",
    "sort-by": "^1.2.0"
  },
  "devDependencies": {
    "@types/mapbox-gl": "^2.7.17",
    "@types/react": "^18.2.15",
    "@types/react-dom": "^18.2.7",
