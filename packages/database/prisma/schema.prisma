generator client {
  provider = "prisma-client-js"
}

generator zod {
  provider = "zod-prisma-types"
  output   = "../node_modules/.prisma/zod"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model NkCredential {
  firstName    String
  lastName     String
  userId       Int      @id
  ownTeamId    Int
  accessToken  String
  refreshToken String
  tokenExpiry  DateTime
}
