{
  "name": "database",
  "version": "0.0.0",
  "type": "module",
  "exports": {
    ".": "./src/client.ts",
    "./zod": "./src/zod.ts"
  },
  "files": [
    "prisma/**",
    "src/**"
  ],
  "scripts": {
    "dev": "docker run -p 5432:5432 --env-file ./.env.development.local -e PGDATA=/var/lib/postgresql/data/pgdata --volume wpi-rowing-dash-db:/var/lib/postgresql/data postgres:latest",
    "db:format": "prisma format",
    "db:push": "dotenv -e .env.development.local -- prisma db push --skip-generate",
    "db:migrate:dev": "dotenv -e .env.development.local -- prisma migrate dev",
    "db:migrate:reset": "dotenv -e .env.development.local -- prisma migrate reset",
    "db:generate": "prisma generate",
    "db:seed": "dotenv -e .env.development.local -- prisma db seed",
    "typecheck": "tsc",
    "lint": "eslint --max-warnings 0 --report-unused-disable-directives --fix ."
  },
  "prisma": {
    "seed": "node --loader ts-node/esm prisma/seed.ts"
  },
  "dependencies": {
    "@prisma/client": "^5.5.2",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@swc/core": "^1.3.60",
    "@swc/helpers": "^0.5.1",
    "@types/node": "^20.2.3",
    "axios": "^1.6.1",
    "dotenv": "^16.3.1",
    "dotenv-cli": "^7.3.0",
    "eslint-config-custom": "workspace:*",
    "prisma": "^5.5.2",
    "puppeteer": "^21.5.1",
    "ts-node": "^10.9.1",
    "tsconfig": "workspace:*",
    "typescript": "^5.0.2",
    "zod-prisma-types": "^3.1.0"
  }
}
