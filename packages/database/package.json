{
  "name": "database",
  "version": "0.0.0",
  "type": "module",
  "exports": {
    ".": "./src/client.ts",
    "./zod": "./src/zod.ts"
  },
  "files": [
    "prisma/**",
    "src/**"
  ],
  "scripts": {
    "dev": "docker run -d postgres:latest -p 5432:5432 --env-file .env.development.local",
    "db:format": "prisma format",
    "db:push": "prisma db push --skip-generate",
    "db:migrate:dev": "prisma migrate dev",
    "db:migrate:reset": "prisma migrate reset",
    "db:generate": "prisma generate",
    "db:seed": "prisma seed",
    "typecheck": "tsc",
    "lint": "eslint --max-warnings 0 --report-unused-disable-directives --fix ."
  },
  "prisma": {
    "seed": "node --loader ts-node/esm prisma/seed.ts"
  },
  "dependencies": {
    "@prisma/client": "^5.5.2",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@swc/core": "^1.3.60",
    "@swc/helpers": "^0.5.1",
    "@types/node": "^20.2.3",
    "eslint-config-custom": "workspace:*",
    "prisma": "^5.5.2",
    "ts-node": "^10.9.1",
    "tsconfig": "workspace:*",
    "typescript": "^5.0.2",
    "zod-prisma-types": "^3.1.0"
  }
}
